<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CheckListFragment">

    <!-- TODO: Update blank fragment layout -->
    <ImageView
        android:id="@+id/imv_bckgevent"
        android:layout_width="412dp"
        android:layout_height="191dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/bckgevent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_pom"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_pom"
        android:layout_width="170dp"
        android:layout_height="27dp"
        android:layout_marginTop="25dp"
        android:paddingTop="3dp"
        android:text="@string/pom"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.032"
        app:layout_constraintStart_toEndOf="@+id/back_check"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/img_notif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="37dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="25dp"
        android:src="@drawable/notifikasi"
        tools:layout_editor_absoluteX="265dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/back_check"
        android:layout_width="32dp"
        android:layout_height="26dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/v_detail"
        android:layout_width="408dp"
        android:layout_height="3dp"
        android:background="@color/grey"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_detail"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_detail"
        android:layout_width="47dp"
        android:layout_height="24dp"
        android:layout_marginTop="8dp"
        android:text="Detail"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/back_check"
        app:layout_constraintTop_toBottomOf="@+id/imv_bckgevent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_panitia"
        android:layout_width="53dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:text="Panitia"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/tv_detail"
        app:layout_constraintTop_toTopOf="@+id/tv_detail"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_agenda"
        android:layout_width="57dp"
        android:layout_height="23dp"
        android:layout_marginStart="10dp"
        android:text="Agenda"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/tv_panitia"
        app:layout_constraintTop_toTopOf="@+id/tv_panitia"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_anggaran"
        android:layout_width="65dp"
        android:layout_height="20dp"
        android:layout_marginStart="10dp"
        android:text="Anggaran"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/tv_agenda"
        app:layout_constraintTop_toTopOf="@+id/tv_agenda"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_chekclist"
        android:layout_width="66dp"
        android:layout_height="26dp"
        android:layout_marginStart="10dp"
        android:text="Checklist"
        android:textColor="@color/blue"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/tv_anggaran"
        app:layout_constraintTop_toTopOf="@+id/tv_anggaran"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_ketua"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Perlengkapan Utama"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/back_check"
        app:layout_constraintTop_toBottomOf="@+id/v_detail"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_lihatdetail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="Lihat Detail"
        android:textColor="@color/blue"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/tv_chekclist"
        app:layout_constraintTop_toBottomOf="@+id/v_detail" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@drawable/border_blue"
        android:src="@drawable/check_blue"
        app:layout_constraintStart_toStartOf="@+id/tv_ketua"
        app:layout_constraintTop_toBottomOf="@+id/tv_ketua"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view7"
        android:layout_width="24dp"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/border_text"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view8"
        android:layout_width="24dp"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/border_text"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/view7"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/view9"
        android:layout_width="24dp"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/border_text"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/view8"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:background="@drawable/border_blue"
        android:src="@drawable/check_blue"
        app:layout_constraintStart_toStartOf="@+id/textView30"
        app:layout_constraintTop_toBottomOf="@+id/textView30"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:text="Perlengkapan Promosi"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/view9"
        app:layout_constraintTop_toBottomOf="@+id/view9" />

    <TextView
        android:id="@+id/tv_lihatdetail2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lihat Detail"
        android:textColor="@color/blue"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/tv_lihatdetail"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView30"
        app:layout_constraintTop_toTopOf="@+id/textView30" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:background="@drawable/border_blue"
        android:src="@drawable/check_blue"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/v_3"
        android:layout_width="24dp"
        android:layout_height="22dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/border_text"
        app:layout_constraintStart_toStartOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        tools:ignore="MissingConstraints" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:layout_marginStart="8dp"
        android:text="Bola Basket"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toTopOf="@+id/imageView3" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bola Sepak"
        android:layout_marginTop="3dp"
        android:layout_marginStart="10dp"
        android:textColor="@color/dark_grey"
        app:layout_constraintStart_toEndOf="@+id/view7"
        app:layout_constraintTop_toTopOf="@+id/view7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bola Voli"
        android:layout_marginTop="3dp"
        android:layout_marginStart="10dp"
        android:textColor="@color/dark_grey"
        app:layout_constraintStart_toEndOf="@+id/view8"
        app:layout_constraintTop_toTopOf="@+id/view8" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="3dp"
        android:text="Trofi dan Mendali"
        android:textColor="@color/dark_grey"
        app:layout_constraintStart_toEndOf="@+id/view9"
        app:layout_constraintTop_toTopOf="@+id/view9" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Spanduk Acara"
        android:layout_marginTop="3dp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/imageView4" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Poster Promosi"
        android:layout_marginTop="3dp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toTopOf="@+id/imageView6"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="3dp"
        android:text="Brosur/Flyer"
        android:textColor="@color/dark_grey"
        app:layout_constraintStart_toEndOf="@+id/v_3"
        app:layout_constraintTop_toTopOf="@+id/v_3" />

    <include
        layout="@layout/fragment_check1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/tv_lihatdetail2"
        app:layout_constraintTop_toBottomOf="@+id/textView35" />

</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>