<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailEvent2Fragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/tv_panitia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:text="Panitia"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/et_ktgriacara"
        android:layout_width="330dp"
        android:layout_height="38dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/tv_white"
        android:paddingStart="8dp"
        android:paddingTop="10dp"
        android:text="84 Panitia"
        app:layout_constraintStart_toStartOf="@+id/tv_panitia"
        app:layout_constraintTop_toBottomOf="@+id/tv_panitia"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:layout_width="25dp"
        android:layout_height="27dp"
        android:layout_marginTop="6dp"
        android:layout_marginEnd="5dp"
        android:src="@drawable/next"
        app:layout_constraintEnd_toEndOf="@+id/et_ktgriacara"
        app:layout_constraintTop_toTopOf="@+id/et_ktgriacara" />

    <TextView
        android:id="@+id/tv_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:text="Informasi Lainnya"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/et_ktgriacara"
        app:layout_constraintTop_toBottomOf="@+id/et_ktgriacara" />


    <TextView
        android:id="@+id/textView44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="@string/info_lainnya"
        android:textColor="@color/dark_grey"
        app:layout_constraintStart_toStartOf="@+id/tv_info"
        app:layout_constraintTop_toBottomOf="@+id/tv_info"
        tools:ignore="MissingConstraints" />

    <View
        android:layout_width="411dp"
        android:layout_height="88dp"
        app:layout_constraintTop_toBottomOf="@+id/textView44"
        tools:layout_editor_absoluteX="0dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>